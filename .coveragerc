#
[run]
branch = True
source =
    llm_utils
    interactive
    batch_runner
    llm_runner

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
    except ImportError
    raise NotImplementedError
    pass
omit =
    tests/*
    */tests/*
    */__init__.py

[html]
directory = coverage_html_report

